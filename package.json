{
  "name": "@xano/cli",
  "description": "CLI for Xano's Metadata API",
  "version": "0.0.35",
  "author": "Sean Montgomery",
  "bin": {
    "xano": "./bin/run.js"
  },
  "bugs": "https://github.com/xano-inc/cli/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "inquirer": "^8.2.5",
    "js-yaml": "^4.1.0",
    "lodash.snakecase": "^4.1.1",
    "open": "^11.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/inquirer": "^8.2.5",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.snakecase": "^4.1.9",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/xano-inc/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "xano",
    "dirname": "xano",
    "commands": "./dist/commands",
    "helpClass": "./dist/help",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "branch": {
        "description": "Manage workspace branches"
      },
      "function": {
        "description": "Manage reusable functions in a workspace"
      },
      "profile": {
        "description": "Manage CLI profiles and authentication"
      },
      "run": {
        "description": "Execute XanoScript and manage Xano Run projects"
      },
      "run:env": {
        "description": "Manage environment variables for a project"
      },
      "run:projects": {
        "description": "Manage Xano Run projects"
      },
      "run:secrets": {
        "description": "Manage secrets for a project"
      },
      "run:sessions": {
        "description": "Manage Xano Run sessions"
      },
      "run:sink": {
        "description": "Retrieve sink data from completed sessions"
      },
      "static_host": {
        "description": "Manage static hosting sites"
      },
      "static_host:build": {
        "description": "Manage static host builds"
      },
      "release": {
        "description": "Manage releases in a workspace"
      },
      "tenant": {
        "description": "Manage tenants in a workspace"
      },
      "tenant:backup": {
        "description": "Manage tenant backups"
      },
      "unit_test": {
        "description": "Manage and run unit tests"
      },
      "workflow_test": {
        "description": "Manage and run workflow tests"
      },
      "workspace": {
        "description": "Manage Xano workspaces"
      }
    }
  },
  "repository": "xano-inc/cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "./bin/dev.js",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
